{
    "name": "jasonlam604/phpinfocli",
    "type": "project",
    "description": "PhpInfoCLI is phpinfo dump ASCII user interface dump at command line",
    "keywords": ["laravel","library","phpinfo"],
    "license": "MIT",
    "homepage": "https://github.com/jasonlam604/PhpInfoCLI",
    "authors": [
        {
            "name": "Jason Lam",
            "email": "jasonlam604@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "jasonlam604/stringizer" : "^2.2.0"
        
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "PhpInfoCLI\\": "src"
        }
    },
    "extra": {
        "phar-builder": {
            "entry-point": "./src/index.php",
            "compression": "GZip",
            "name": "phpinfocli.phar",
            "output-dir": "./bin",
            "include": []
        }
    }
}
